{
    "name": "jobsearch extention",
    "version": "0.1.0",
    "manifest_version": 2,
    "background":  {
        "scripts": ["./background/dist/main.js"]
    },
    "options_page": "./options.html",
    "browser_action": {
        "default_popup": "./background/dist/index.html"
    },
    "permissions": [
        "tabs",
        "storage",
        "https://www.google.com/*",
        "https://www.linkedin.com/*"
    ]
}